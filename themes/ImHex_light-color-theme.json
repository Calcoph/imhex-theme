        "editorCursor.foreground": "#000000",
        "editorError.background": "#FF1000A0",
        "debugIcon.breakpointForeground": "#0080F080",
        "editorLineNumber.foreground": "#005050",
        "editorLineNumber.activeForeground": "#005050",
        "editor.lineHighlightBackground": "#00000040",
        "editor.lineHighlightBorder": "#00000040",
        "editor.inactiveSelectionBackground": "#80808040",//
        "activityBar.activeBackground": "#98BAE1",
        "activityBar.activeBorder": "#558bc9",
        "activityBar.background": "#DBDBDB",
        "activityBar.foreground": "#20408C",
		"activityBarBadge.background": "#98BAE1",
        "sideBar.background": "#C3C3C3",
        "sideBarSectionHeader.background": "#C7DEFA",
        "badge.background": "#BDD1E9",
        "button.background": "#BDD1E9",
        "dropdown.background": "#FFFFFF",
        "editor.findMatchBackground": "#3A3A52",  
        "editor.selectionBackground": "#00006080",
        "editorGroup.dropBackground": "#1D355054",
        "editorGroupHeader.tabsBackground": "#C3C3C3",
        "editorWidget.background": "#FFFFFF",
        "focusBorder": "#98BAE1",
        "input.background": "#FFFFFF",
        "inputOption.activeBorder": "#98BAE1",
        "panel.background": "#FFFFFF",
		"panel.border": "#999999",
        "panelTitle.activeBorder": "#98BAE1",
        "selection.background": "#BDDAFD",
        "statusBar.background": "#F0F0F0",
        "statusBar.foreground": "#000000",
        "tab.activeBorder": "#98BAE1",
        "tab.border": "#DBDBDB",
        "tab.activeBackground": "#98BAE1",
        "tab.inactiveBackground": "#BDD1E9",
        "titleBar.activeBackground": "#DBDBDB",
        "titleBar.inactiveBackground": "#DBDBDB",
        "sideBar.border": "#999999",
        "activityBar.border": "#999999",
        "editorGroup.border": "#999999",
        "editorGroupHeader.tabsBorder": "#98BAE1"
