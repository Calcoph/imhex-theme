	"tokenColors": [
        {
            "name": "Default",
            "scope": [
                "keyword.operator",
                "variable.language.dollar.hexpat",
                "source"
            ],
            "settings": {
                "foreground": "#7f7f7f"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#060CFF"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "name": "String",
            "scope": [
                "string.quoted.double"
            ],
            "settings": {
                "foreground": "#A02020"
            }
        },
        {
            "name": "Char literal",
            "scope": [
                "string.quoted.double"
            ],
            "settings": {
                "foreground": "#704030"
            }
        },
        {
            "name": "Punctuation",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Preprocessor",
            "scope": [
                "meta.preprocessor"
            ],
            "settings": {
                "foreground": "#606040"
            }
        },
        {
            "name": "Identifier",
            "scope": [
                "variable",
                "storage.type",
                "variable.language",
                "entity.name"
            ],
            "settings": {
                "foreground": "#404040"
            }
        },
        {
            "name": "Known identifier",
            "scope": [
              "storage.type.signed",
              "storage.type.unsigned",
              "storage.type.float",
              "storage.type.special",
              "storage.modifier"
            ],
            "settings": {
                "foreground": "#106060"
            }
        },
        {
            "name": "Preproc identifier",
            "settings": {
                "foreground": "#A040C0"
            }
        },
        {
            "name": "Comment single line",
            "scope": [
                "comment.line"
            ],
            "settings": {
                "foreground": "#205020"
            }
        },
        {
            "name": "Comment multi line",
            "scope": [
                "comment.block"
            ],
            "settings": {
                "foreground": "#205040"
            }
        },
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Tag Attribute, Embedded",
			"scope": [
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
